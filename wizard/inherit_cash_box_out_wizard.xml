<odoo>
    <record id="view_inherited_cash_box_out" model="ir.ui.view">
        <field name="name">cash.box.out.inherit</field>
        <field name="model">cash.box.out</field>
        <field name="inherit_id" ref="account.cash_box_out_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="before">
                <field name="category_id" class="oe_inline" options="{'no_create': True, 'no_edit': True, 'no_open': True}"/>
            </xpath>
        </field>
    </record>
</odoo>
