<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_expense_detail_wizard_form" model="ir.ui.view">
            <field name="name">expense.detail.wizard.form</field>
            <field name="model">expense.detail.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="expense_journal_lines_ids" nolabel="1">
                            <tree editable="false" create="false" delete="false">
                                <field name="date"/>
                                <field name="name"/>
                                <field name="category_id"/>
                                <field name="pos_session_id"/>
                                <field name="amount" sum="Total"/>
                            </tree>
                        </field>
                    </group>

                    <footer>
                        <button string="OK" class="btn-primary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>